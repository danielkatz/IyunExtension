{
  "manifest_version": 2,

  "name": "Iyun Sefaria Extension",
  "description": "This extension augments the sefaria experiance with Iyun suggestions",
  "version": "1.0",

  "permissions": [
    "https://localhost:44360/"
  ],
  "content_scripts": [
    {
      "matches": [ "https://*/*" ],
      "js": [ "jquery-3.2.1.min.js", "safaria.js" ],
      "run_at": "document_idle"
    }
  ]
}